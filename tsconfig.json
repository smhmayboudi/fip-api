{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap":false,
    "strict": true,
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017",
      "es2017.intl",
      "es2017.object",
      "es2017.sharedmemory",
      "es2017.string",
      "es2017.typedarrays",
      "scripthost"
    ],
    "paths": {
      "@fip/apm": [
        "libs/apm/src"
      ],
      "@fip/apm/*": [
        "libs/apm/src/*"
      ],
      "@fip/common": [
        "libs/common/src"
      ],
      "@fip/common/*": [
        "libs/common/src/*"
      ],
      "@fip/dgraph": [
        "libs/dgraph/src"
      ],
      "@fip/dgraph/*": [
        "libs/dgraph/src/*"
      ],
      "@fip/eventstore": [
        "libs/eventstore/src"
      ],
      "@fip/eventstore/*": [
        "libs/eventstore/src/*"
      ],
      "@fip/prom": [
        "libs/prom/src"
      ],
      "@fip/prom/*": [
        "libs/prom/src/*"
      ],
      "@fip/sentry": [
        "libs/sentry/src"
      ],
      "@fip/sentry/*": [
        "libs/sentry/src/*"
      ]
    }
  },
  "exclude": [
    ".yarn",
    "coverage",
    "dist",
    "node_modules",
    "yarn-packages-offline-cache"
  ]
}